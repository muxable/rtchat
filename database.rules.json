{
  "rules": {
    "pullables": {
      "$pullKey": {
        ".read": true,
        ".write": false
      }
    },
    "streamers": {
      ".indexOn": ["activeSessionId"],
      ".read": "query.orderByChild == 'activeSessionId' && query.startAt == '\u0000' && query.endAt == '\uFFFF'",
      ".write": false,
      "$userId": {
        ".read": true,
        ".write": false
      }
    },
    "sessions": {
      "$userId": {
        ".read": "$userId === auth.uid",
        ".write": false
      }
    },
    "sentinel": {
      ".read": true,
      ".write": false
    }
  }
}
